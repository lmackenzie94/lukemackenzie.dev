{
  "extends": "astro/tsconfigs/strict",
  // // dont fail build on error
  // "compilerOptions": {
  //   "noEmitOnError": false
  // }
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@pages/*": ["src/pages/*"],
      "@utils/*": ["src/utils/*"],
      "@assets/*": ["src/assets/*"]
    }
  }
}
